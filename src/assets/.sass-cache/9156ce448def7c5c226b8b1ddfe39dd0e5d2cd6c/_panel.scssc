// TODO: Remove namespacing when old zanata removed
.new-zanata {

.panel {
  @extend .bg--pop-high;
  position: relative;
}

.panel__header {
  @extend .bg--pop-highest;
  border-width: 0 0 2px 0;
  // display: table;
  width: 100%;
  position: relative;
  max-height: rhythm(2.05);
}

.panel__sub-header {
  @extend .bg--pop-higher;
  border-width: 0 0 2px 0;
}

.panel__heading {
  @include adjust-font-size-to(16px, 2);
  @include ellipsis;
  padding: 0 rhythm(.5, 16px);
  font-weight: 400;
  overflow: hidden;
  margin: 0;
  @include breakpoint($gte-medium) {
    @include adjust-font-size-to(18px, 2);
    padding: 0 rhythm(.5, 18px);
  }
}

.panel__header__info {
  @include ellipsis;
  padding: rhythm(.45) rhythm(.5);
  overflow: hidden;
  margin: 0;
}

.panel__heading--small {
  @include adjust-font-size-to(14px);
  @include ellipsis;
  padding: 0 rhythm(.5, 14px);
  font-weight: 400;
  overflow: hidden;
  margin: 0;
  text-transform: uppercase;
}

.panel__header__sort,
.panel__header__actions {
  float: right;
  white-space: nowrap;
  // margin: rhythm(.45) rhythm(.5) 0 0;
}

.panel__header__back {
  height: rhythm(2);
  width: rhythm(1.5);
  text-align: center;
  display: inline-block;
  position: absolute;
  left: 0;
  top: 0;
  padding: rhythm(.5) 0;
  i:before {
    height: rhythm(2);
  }
  + .panel__heading {
    padding-left: rhythm(1.5, 16px);
  }
  @include breakpoint($gte-small) {
    + .panel__heading {
      padding-left: rhythm(1.5, 18px);
    }
  }
  @include breakpoint($gte-medium) {
    display: none;
    + .panel__heading {
      padding-left: rhythm(.5, 18px);
    }
  }
}

.panel__search {
  display: block;
  &:before {
    left: rhythm(.425);
    top: rhythm(0);
    line-height: rhythm(1);
  }
  input {
    @include adjust-font-size-to(14px, .8);
    padding: rhythm(.125, 14px) rhythm(2, 14px) rhythm(.125, 14px) rhythm(1.375, 14px);
    height: rhythm(1.05, 14px);
    border-radius: 0;
    border: none;
    border-bottom: $default-border;
    margin: 0;
    &:focus,
    &:hover {
      border: none;
      border-bottom: $default-border;
    }
  }
}

.panel__results {
  @extend .l--cf-of;
  position: relative;
  padding: 0 rhythm(.25) 0 rhythm(1.375);
  .txt--meta {
    line-height: inherit;
  }
}

.panel__results--bottom {
  position: absolute;
  bottom: 0;
  width: 100%;
}

.panel__results__type {
  position: absolute;
  top: rhythm(.25, 14px);
  left: rhythm(.5, 14px);
  @include adjust-font-size-to(14px);
}

.panel__search__reset,
.panel__search__button {
  vertical-align: inherit;
}

.panel__search__button {
  margin-left: rhythm(.125, 14px) !important;
  font-weight: 400;
}

.panel__search__cancel {
  @include adjust-font-size-to(14px);
  position: absolute;
  top: 0;
  right: rhythm(.5,14px);
  font-weight: 400;
}

.panel__pager {
  @extend .list--horizontal;
  height: rhythm(1);
  float: right;
  li {
    vertical-align: top;
    margin: 0 0 0 rhythm(-.125);
  }
}

.panel__pager__button {
  line-height: rhythm(1);
  height: rhythm(1);
  width: rhythm(1);
  padding: 0;
  border: none;
  background-color: transparent;
  color: $color-link;
  @include box-shadow(none);
  &:hover,
  &:focus,
  &:active {
    color: $color-link-hover;
    background-color: $color-bg;
    @include box-shadow(none);
  }
  &:active {
    color: $color-link-active;
  }
}

